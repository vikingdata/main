     %echo Generating a basic OpenPGP key
     Key-Type: DSA
     Key-Length: 1024
     Subkey-Type: ELG-E
     Subkey-Length: 1024
     Name-Real: Nobody
     Name-Comment: with stupid passphrase
     Name-Email: nobody@nowhere
     Expire-Date: 0
     Passphrase: BadPhrase
     %pubring Bad.pub
     %secring Bad.sec
     # Do a commit here, so that we can later print "done" :-)
     %commit
     %echo done

